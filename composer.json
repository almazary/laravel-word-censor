{
    "name": "almazary/laravel-word-censor",
    "description": "dirty word censor",
    "keywords": [
        "almazary",
        "laravel-word-censor"
    ],
    "homepage": "https://github.com/almazary/laravel-word-censor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Almazari",
            "email": "almazary@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Almazary\\LaravelWordCensor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Almazary\\LaravelWordCensor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Almazary\\LaravelWordCensor\\LaravelWordCensorServiceProvider"
            ],
            "aliases": {
                "LaravelWordCensor": "Almazary\\LaravelWordCensor\\LaravelWordCensorFacade"
            }
        }
    }
}
